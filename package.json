{
  "name": "vueslim",
  "version": "0.0.1",
  "private": true,
  "description": "A very simple  vue+express application, ready to deploy",
  "author": "Daniel Schmitz <danieljfa@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
